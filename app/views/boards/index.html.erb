<div class="title">All Boards</div>
<div class="container">

<% @boards.each do |board| %>
  <div class="card">
    <h2 class="card-title"><%= board.name %></h2>
    <p class="card-content"><%= board.description %></p>
  </div>
<% end %>

  <div class="add-board-card">
    <h2 class="add-board-text">+ Add new board</h2>

</div>

